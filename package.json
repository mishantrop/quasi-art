{
  "name": "quasi-art.ru",
  "version": "1.0.2",
  "description": "Web Developer Portfolio",
  "scripts": {
    "css:build": "gulp build",
    "css:watch": "gulp watch",
    "js:analyze": "webpack --progress --env ANALYZE=1 --mode=development",
    "js:analyze:report": "webpack --progress --env ANALYZE=1 ANALYZENEEDREPORT=1 ANALYZERMODE='static' --mode=development",
    "js:build": "webpack --progress --mode=production",
    "js:lint": "./node_modules/.bin/eslint assets/templates/main/js/src/**",
    "js:watch": "webpack --watch --progress --mode=development",
    "start": "npm-run-all --parallel js:watch css:watch",
    "build": "npm run js:build && npm run css:build",
    "css:lint": "stylelint assets/templates/main/postcss/**/*.css",
    "css:lint:fix": "stylelint assets/templates/main/postcss/**/*.css --fix",
    "prepare": "husky install"
  },
  "author": "mishantrop",
  "browserslist": [
    "last 2 version",
    "> 1%"
  ],
  "devDependencies": {
    "@babel/core": "7.21.4",
    "@babel/plugin-proposal-class-properties": "7.18.6",
